19th programme
#include <stdio.h>

int main() {
    // Write C code here to test do-while loop
 int i=1;
  do{
     printf("i=%d\n",i);
     i++;
      
  }
 while(i<=5);
 return 0;
 i++;
}







20th programme
#include <stdio.h>

int main() {
    // Write C code here to find sum of n numbers
 int n,i,sum=0;
 printf("enter n:\n");
 scanf("%d",&n);

 for (i=1 ; i<=n; i++)
 {sum=sum+i;
 }
 printf("Sum of %d numbers is %d",n,sum);
return 0;
}


21th programme
#include <stdio.h>

int main() {
    // Write C code here to test while loop
 int i=1,n,sum=0;

 printf("enter n \n",i);
 scanf("%d",&n);
 
 while(i<=n)
 {sum=sum+i;
 i++;
 }
 printf("Sum of %d numbers is %d",n,sum);
return 0;
  
    




}

#22th programme
#include <stdio.h>

int main() {
    // Write C code here to make a multiplication table of any number
 int n,i;
 printf("enter the table");
 scanf("%d",&n);
  for(i=1;i<=10;i++)
  {
printf("%d*%d=%d\n",n,i,n*i);
 
  }
 
 return 0;
}

#include <stdio.h>

int main() {
    // Write C code here to make a multiplication table of any number
 int n,i,fact=1;
 printf("factorial");
 scanf("%d",&n);
  for(i=n;i>=1;i--)
  {fact=fact*i;
  }

  printf("factorial of %d is %d",n,fact);
 

 return 0;
}


