#22th programme
#include <stdio.h>

int main() {
    // Write C code here to make a multiplication table of any number
 int n,i;
 printf("enter the table");
 scanf("%d",&n);
  for(i=1;i<=10;i++)
  {
printf("%d*%d=%d\n",n,i,n*i);
 
  }
 
 return 0;
}

#include <stdio.h>

int main() {
    // Write C code here to make a multiplication table of any number
 int n,i,fact=1;
 printf("factorial");
 scanf("%d",&n);
  for(i=n;i>=1;i--)
  {fact=fact*i;
  }

  printf("factorial of %d is %d",n,fact);
 

 return 0;
}


