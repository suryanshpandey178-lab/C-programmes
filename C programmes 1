#1st programme
#include <stdio.h>

void main() {
    // Write C code to test all the arthemetic operations
   int a,b,sum,sub,multi,div,mod;
   printf("enter first number 'a' and the second number 'b':\n");
   scanf("%d%d",&a,&b);
   a=10,b=5;
   sum=a+b;
   sub=a-b;
   multi=a*b;
   div=a/b;
   mod=a%b;
   printf("sum=%d,sub=%d,multi=%d,div=%d,mod=%d",sum,sub,multi,div,mod);
}


output
enter first number 'a' and the second number 'b':
10
5
sum=15,sub=5,multi=50,div=2,mod=0
//shorter code
