#5th programme
#include <stdio.h>

int main() {
    // Write C code here to test a combination of relational and logical operators
    //case 1
   int a=10,b=5;
   printf("(a>b)||(a==b)=%d",(a>b)||(a==b));
   printf("(a<b)&&(a=b)=%d",(a<b)&&(a=b));
   printf("!(a>=b)=%d",!(a>=b));
    return 0;
}
output:
(a>b)||(a==b)=1(a<b)&&(a=b)=0!(a>=b)=0








