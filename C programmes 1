   printf("(a>b)?a:b=%d",c=(a>b)?a:b);
    return 0;
}







#7th programme
#include <stdio.h>

int main() {
float radius=6, area;

area = 3.14159 * radius * radius;

// Display the result
printf("The area of the circle is: %.2f\n", area);

return 0;
}





#8th programme
#include <stdio.h>

int main() {
    // Write C code here to test bitwise
   int a=5,b=3;
   printf("a&b result is %d\n",a&b);
   printf("a&b result is %d\n",a|b);
   printf("a&b result is %d\n",a^b);
   printf("a&b result is %d\n",a <<1);
   printf("a&b result is %d\n",a>>1);
    return 0;
}
output:a&b result is 1
a&b result is 7
a&b result is 6
a&b result is 10
a&b result is 2






