#8th programme
#include <stdio.h>

int main() {
    // Write C code here to test bitwise
   int a=5,b=3;
   printf("a&b result is %d\n",a&b);
   printf("a&b result is %d\n",a|b);
   printf("a&b result is %d\n",a^b);
   printf("a&b result is %d\n",a <<1);
   printf("a&b result is %d\n",a>>1);
    return 0;
}
output:a&b result is 1
a&b result is 7
a&b result is 6
a&b result is 10
a&b result is 2






