20th programme
#include <stdio.h>

int main() {
    // Write C code here to find sum of n numbers
 int n,i,sum=0;
 printf("enter n:\n");
 scanf("%d",&n);

 for (i=1 ; i<=n; i++)
 {sum=sum+i;
 }
 printf("Sum of %d numbers is %d",n,sum);
return 0;
}


