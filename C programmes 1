#2nd code
#include <stdio.h>

void main() {
    // Write C code to test assignment operator
   int a=6,b=4;
   printf("a=%d,b=%d",a,b);
      int temp;
   temp=a;
   a=b;
   b=temp;
   printf("a=%d,b=%d",a,b);
}
output
a=6,b=4a=4,b=6






#3rd programme
                                                                                                                                                    
#include <stdio.h> 

// Write C code to test increament /decreament
int main() {
	int a = 10;
	printf("value of a = %d\n", a); 
	printf("post increment = %d\n",a++);
	printf("value of a = %d\n", a);
	printf("pre increment = %d\n",++a);
	printf("value of a = %d\n", a);
	printf("post decrement = %d\n", a--);
	printf("value of a = %d\n", a);
	printf("pre decrement = %d\n", --a);
	printf("value of a = %d\n", a);

	return 0;
}
output
value of a = 10
post increment = 10
value of a = 11
pre increment = 12
value of a = 12
post decrement = 12
value of a = 11
pre decrement = 10
value of a = 10




